{
  "name": "snapshot-context",
  "version": "2.3.0",
  "description": "A context for zoroaster which enables snapshot testing.",
  "main": "build/index.js",
  "module": "src/index.js",
  "scripts": {
    "t": "zoroaster -a",
    "test": "yarn t test/spec",
    "test-build": "ALAMODE_ENV=test-build yarn test",
    "test-all": "yarn-s test test-build",
    "b": "alamode src -o build -s",
    "doc": "NODE_DEBUG=doc doc -o README.md",
    "example": "zoroaster -a -t 9999999 example/spec "
  },
  "files": [
    "build",
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/contexttesting/snapshot-context.git"
  },
  "keywords": [
    "zoroaster",
    "context",
    "test",
    "snapshot"
  ],
  "author": "Anton <anton@adc.sh>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/contexttesting/snapshot-context/issues"
  },
  "homepage": "https://github.com/contexttesting/snapshot-context#readme",
  "devDependencies": {
    "alamode": "^2.0.0",
    "documentary": "^1.26.2",
    "spawncommand": "^2.2.0",
    "temp-context": "^2.1.3",
    "yarn-s": "1.1.0",
    "zoroaster": "^3.13.0"
  },
  "dependencies": {
    "@wrote/ensure-path": "^1.0.8",
    "@wrote/read": "^1.0.4",
    "@wrote/write": "^1.0.4",
    "@zoroaster/deep-equal": "^1.0.1",
    "erotic": "^2.1.1",
    "erte": "^1.2.0",
    "frame-of-mind": "^1.2.3",
    "reloquent": "^1.3.0"
  }
}
